  </action>
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="meta Y" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="ShowErrorDescription">
    <keyboard-shortcut first-keystroke="meta F1" />
